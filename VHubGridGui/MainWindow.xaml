<Window x:Class="VHubGridGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VHubGridGui"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addRouterButton" Content="Add Router" HorizontalAlignment="Left" Margin="330,140,0,0" VerticalAlignment="Top" Click="button_Click" Height="150" Width="149"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="330,340,0,0" TextWrapping="Wrap" Text="Result" VerticalAlignment="Top" Width="149" Height="18" TextChanged="textBox_TextChanged"/>

    </Grid>
</Window>
